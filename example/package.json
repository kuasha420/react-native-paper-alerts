{
  "name": "helloworld",
  "version": "1.0.0",
  "private": true,
  "homepage": "/",
  "scripts": {
    "android": "react-native run-android --main-activity com.zoontek.rnbootsplash.RNBootSplashActivity",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "regenerate-bootsplash": "react-native generate-bootsplash src/assets/bootsplash_logo_original.png --background-color=ffffff --logo-width=200 --assets-path=src/assets --webroot-path=web",
    "postversion": "react-native-version",
    "web:prepare": "rm -rf package && cp -r ../lib/module/ package",
    "web": "yarn web:prepare && webpack serve --open",
    "web:build": "yarn web:prepare && webpack --mode=\"production\"",
    "web:serve": "serve"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.4",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "localforage": "^1.9.0",
    "mobx": "^6.3.1",
    "mobx-react-lite": "^3.2.0",
    "mobx-state-tree": "^5.0.1",
    "mst-persistent-store": "^1.2.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-bootsplash": "git+https://github.com/kuasha420/react-native-bootsplash.git",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-immersive-bars": "1.0.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-paper": "^4.9.2",
    "react-native-paper-toast": "^1.1.1",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.2.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web": "^0.16.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react-native": "^0.64.10",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/webpack-dev-server": "^3.11.5",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "^0.16.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.14.0",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "process": "^0.11.10",
    "react-native-version": "^4.0.0",
    "react-refresh": "^0.10.0",
    "react-test-renderer": "17.0.1",
    "serve": "^12.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^3.8.3",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
